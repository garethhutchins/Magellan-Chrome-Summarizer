    "manifest_version": 3,
    "permissions": ["scripting"],
    "host_permissions": [
      "<all_urls>"
    ],
    "action" : {
      "default_title" : "Summarise Current Web Page",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }